      "Nip\\Cookie\\Tests\\": "tests/",
      "Nip\\Cookie\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0|^8.0",
    "symfony/http-foundation": "^5.0|^6.0|^7.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
