{
  "name": "bytic/cookie",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/cookie",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Cookie\\": "src"
    }
  },
  "autoload-dev": {
